{
  "name": "expense",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "expenseName": {
      "type": "string",
      "required": true
    },
    "expenseType": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "number",
      "required": true
    },
    "expenseDate": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Paid",
      "example": "Paid | Due"
    },
    "employeeId": {
      "type": "string",
      "required": true
    },
    "attachment": {
      "type": [
        "string"
      ],
      "required": true
    },
    "additionalNotes": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
