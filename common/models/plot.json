    "plotcategories": {
      "type": "belongsTo",
      "model": "plotcategory",
      "foreignKey": ""
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
